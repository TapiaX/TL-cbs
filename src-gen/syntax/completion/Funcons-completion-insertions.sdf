    COMPLETION-INSERTION -> "|"    {literal-completion}
    COMPLETION-INSERTION -> "&"    {literal-completion}
    COMPLETION-INSERTION -> "&"    {literal-completion}
    COMPLETION-INSERTION -> "("    {literal-completion}
    COMPLETION-INSERTION -> ")"    {literal-completion}
    COMPLETION-INSERTION -> "["    {literal-completion}
    COMPLETION-INSERTION -> "]"    {literal-completion}
    COMPLETION-INSERTION -> "{"    {literal-completion}
    COMPLETION-INSERTION -> ","    {literal-completion}
    COMPLETION-INSERTION -> "}"    {literal-completion}
    COMPLETION-INSERTION -> "|->"  {literal-completion}

exports
  lexical syntax
     -> COMPLETION-INSERTION 